<!-- FOOOTER  -->
<div id="footer">
  <div class="blog--footer__back"></div>
  <div class="blog--footer__mode-select-panel">
    <div class="blog--footer__mode-select--item1"></div>
    <div class="blog--footer__mode-select--item2"></div>
    <div class="blog--footer__mode-select--item3"></div>
    <div class="blog--footer__mode-select--item4"></div>
    <div class="blog--footer__user-info--panel">
      <div class="blog--footer__user-info--img"></div>
      <div class="blog--footer__user-info--innerpanel">
        <div class="blog--footer__user-info--name">
          <% if (user) { %>
            <%= user.name %>
          <% } else { %>
            Anonymous
          <% } %>
        </div>
        <div class="blog--footer__user-info--performence">
          <% if (user) { %>
            AddClassExp: <%= user.addClassExp %>pp
          <% } else { %>
            0pp
          <% } %>
        </div>
        <div class="blog--footer__user-info--accurcy">
          <% if (user) { %>
            Accurcy: 99.99%
          <% } else { %>
            0%
          <% } %>
        </div>
        <div class="blog--footer__user-info--level">
          <% if (user) { %>
            lv 104
          <% } else { %>
            lv 0
          <% } %>
        </div>
      </div>
      <div class="blog--footer__user-info--rank">
        <% if (user) { %>
          #1
        <% } else { %>
          #99999
        <% } %>
      </div>
    </div>
  </div>
</div>
</body>
</html>